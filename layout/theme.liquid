<!doctype html>

<script src="https://code.jquery.com/jquery-3.6.3.js" integrity="sha256-nQLuAZGRRcILA+6dMBOvcRh5Pe310sBpanc6+QBmyVM=" crossorigin="anonymous"></script>

<!-- Left Banner Auto Scroll -->
<script>
    setInterval(function () {
      $(document.getElementsByClassName('slick-next')[0]).trigger( "click" );
      $(document.getElementsByClassName('slick-next')[1]).trigger( "click" );
      },4500);
</script>

<html class="no-js wbboxlt" lang="{{ request.locale.iso_code }}" {% if settings.wbboxed %} id="wbboxlayout" {% endif %}>
  <head>
    <meta charset="utf-8"> 
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="">
    <link rel="canonical" href="{{ canonical_url }}">
    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>
    <script type="text/javascript" src="{{ 'crisp.js' | asset_url }}"> in the <head> of your theme.liquid

    {%- if settings.favicon != blank -%}
      <link rel="icon" type="image/png" href="{{ settings.favicon | img_url: '32x32' }}">
    {%- endif -%}

    <title>
      {{ page_title }}
      {%- if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif -%}
      {%- if current_page != 1 %} &ndash; Page {{ current_page }}{% endif -%}
      {%- unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless -%}
    </title>

    {% if page_description %}
      <meta name="description" content="{{ page_description | escape }}">
    {% endif %}

    {% render 'meta-tags' %}

    <script src="{{ 'global.js' | asset_url }}" defer="defer"></script>
    {{ content_for_header }}

    <svg style="display:none">


    <symbol id="wish" viewBox="0 0 96 96" ><path d="M87.598,15.029c-9.373-9.373-24.568-9.373-33.94,0L48,20.686l-5.657-5.656c-9.373-9.373-24.569-9.373-33.941,0  c-9.373,9.373-9.373,24.568,0,33.941l5.657,5.654L48,88.566l33.941-33.941l5.656-5.654C96.971,39.598,96.971,24.402,87.598,15.029z   M70.627,54.627L48,77.254L25.374,54.627L14.061,43.314c-6.249-6.248-6.249-16.379,0-22.629c6.249-6.248,16.379-6.248,22.627,0  L48,32l11.314-11.314c6.248-6.248,16.379-6.248,22.627,0c6.248,6.25,6.248,16.379,0,22.629L70.627,54.627z"/></symbol>

    

  </svg>

    {% style %}
    {%- assign base_font = settings.type_base_font -%}  
      :root {
        --wbbase-font: {{ settings.type_base_font | remove: 'Google+' | replace: '+', ' ' }};
        --font-size-header: {{ settings.type_header_base_size }};
        --font-size-base: {{ settings.type_base_size }};
        
        --gradient-base-accent-1: {% if settings.gradient_accent_1 != blank %}{{ settings.gradient_accent_1 }}{% else %}{{ settings.colors_accent_1 }}{% endif %};
        --color-base-solid-button-labels: {{ settings.colors_solid_button_labels }};

        --gradient-base-accent-2: {% if settings.gradient_accent_2 != blank %}{{ settings.gradient_accent_2 }}{% else %}{{ settings.colors_accent_2 }}{% endif %};
        --color-wb-hovercolor-button-text: {{ settings.wb_hovercolor_button_text }};

        --color-wbmaincolors-text: {{ settings.wbmaincolors_text }};
        --color-base-text: {{ settings.colors_text.red }}, {{ settings.colors_text.green }}, {{ settings.colors_text.blue }};
        --color-wbbordercolor: {{ settings.wbborder_color }};
        
        --color-background: {{ settings.colors_background_1.red }}, {{ settings.colors_background_1.green }}, {{ settings.colors_background_1.blue }};
        --gradient-background: {% if settings.gradient_background_1 != blank %}{{ settings.gradient_background_1 }}{% else %}{{ settings.colors_background_1 }}{% endif %};

        --payment-terms-background-color: {{ settings.colors_background_1 }};

        --color-wbprodct-background: {{ settings.wbprodct_background }};
      }

      *,
      *::before,
      *::after {
        box-sizing: inherit;
      }
      svg{
        width: 14px;height: 14px;
      }
    {% endstyle %} 

    {{ 'bootstrap.min.css' | asset_url | stylesheet_tag }}
    {{ 'slick.css' | asset_url | stylesheet_tag }}
    {{ 'slick-theme.css' | asset_url | stylesheet_tag }}
    {{ 'font-awesome.min.css' | asset_url | stylesheet_tag }}
    {{ 'base.css' | asset_url | stylesheet_tag }}
    {{ 'jquery.fancybox.min.css' | asset_url | stylesheet_tag }}
    {{ 'wbquickview.css' | asset_url | stylesheet_tag }}
    {{ 'component-list-menu.css' | asset_url | stylesheet_tag }}


    {%- if settings.predictive_search_enabled -%}
      <link rel="stylesheet" href="{{ 'component-predictive-search.css' | asset_url }}" media="print" onload="this.media='all'">
    {%- endif -%}

    {% if settings.type_base_font contains 'Google' %}
      <link href="https://fonts.googleapis.com/css?family={{ settings.type_base_font | remove: 'Google+' }}:400,500,600,700"  rel="stylesheet">
    {% endif %}
    <script>document.documentElement.className = document.documentElement.className.replace('no-js', 'js');</script>
  {% render 'minmaxify-head' %}
</head>

  <body class="gradient {% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}">

    <a class="skip-to-content-link button visually-hidden" href="#MainContent">
      {{ "accessibility.skip_to_text" | t }}
    </a>

    {% section 'announcement-bar' %}
    {% section 'header' %}
    <div class="page-container drawer-page-content" id="PageContainer">
      <div class="cate-menu">
        <div class="container">
          <div class="row">
             <div class="col-lg-3 col-md-4 col-12 hleftw d-lg-inline-block d-none">
                
             </div>
             <div class="col-lg-9 col-md-12 col-12 rightw">
                {% section 'cms-menu' %}
             </div>
          </div>
        </div>
      </div>
      <main id="MainContent" class="content-for-layout focus-none" role="main" tabindex="-1">
        <div class="container">
          <div class="row">
            <div class="col-12 col-md-4 col-lg-3 leftw">
                {% section 'site-nav' %}
                {% section 'popular' %}
                {% section 'popular2' %}
                {% section 'bestproduct' %}
                {% section 'secondleftbanner' %}
            </div>
            <div class="col-12 col-md-8 col-lg-9 right-col">
                {% render 'breadcrumbs' %}
                {{ content_for_layout }}
            </div>
          </div>
        </div>
      </main>
      {% section 'logo-bar' %}
      {% section 'popup-products' %}
      {% section 'footer' %}
    </div>
    <ul hidden>
      <li id="a11y-refresh-page-message">{{ 'accessibility.refresh_page' | t }}</li>
    </ul>

    {% render 'wbquick-view' %}

    <script>
      window.shopUrl = '{{ shop.url }}';
      window.routes = {
        cart_add_url: '{{ routes.cart_add_url }}',
        cart_change_url: '{{ routes.cart_change_url }}',
        cart_update_url: '{{ routes.cart_update_url }}',
        predictive_search_url: '{{ routes.predictive_search_url }}'
      };

      window.cartStrings = {
        error: `{{ 'sections.cart.cart_error' | t }}`,
        quantityError: `{{ 'sections.cart.cart_quantity_error_html' | t: quantity: '[quantity]' }}`
      }

      window.variantStrings = {
        addToCart: `{{ 'products.product.add_to_cart' | t }}`,
        soldOut: `{{ 'products.product.sold_out' | t }}`,
        unavailable: `{{ 'products.product.unavailable' | t }}`,
      }

      window.accessibilityStrings = {
        imageAvailable: `{{ 'products.product.media.image_available' | t: index: '[index]' }}`,
        shareSuccess: `{{ 'general.share.success_message' | t }}`,
        pauseSlideshow: `{{ 'sections.slideshow.pause_slideshow' | t }}`,
        playSlideshow: `{{ 'sections.slideshow.play_slideshow' | t }}`,
      }
    </script>

    {%- if settings.predictive_search_enabled -%}
      <script src="{{ 'predictive-search.js' | asset_url }}" defer="defer"></script>
    {%- endif -%}
    
    

    {%comment%} Webibazaar Code {%endcomment%}
    <script src="{{ 'jquery.min.js' | asset_url }}"></script>
    <script src="{{ 'jquery.fancybox.min.js' | asset_url }}"></script>
    <script src="{{ 'popper.min.js' | asset_url }}"></script>
    <script src="{{ 'bootstrap.min.js' | asset_url }}"></script>
    <script src="{{ 'slick.min.js' | asset_url }}"></script>
    <script src="{{ 'wbcustom.js' | asset_url }}"></script>
    <script src="{{ 'countdown.min.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'wbcartdropdown.js' | asset_url }}" ></script>

    {% if template == 'index' %}
        <script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>
    {%- endif -%}
    
    <script src="{{ 'wbquickview.js' | asset_url }}" async></script>

    {% comment %}Webibazaar RTL{% endcomment %}
    {% if settings.wbenblertl or request.locale.iso_code == 'ar' %}
        <link rel="stylesheet" href="{{ 'wbrtl.css' | asset_url }}" type="text/css">
    {% endif %}

    {% if settings.wb_enablewish %}
      {{ 'wishlist.js' | asset_url | script_tag }}  
      <script>
          $(".wbwishremove").on('click', function(){
              $(this).addClass("wbloadtowish wbwishadding");
              setTimeout(function () {
                  $(".wbwishremove").removeClass("wbloadtowish");
              },1000);
              setTimeout(function () {
                  $(".wbwishadding").removeClass("wbwishadding");
              },2000);
          });
      (function() {
          Wishlist.init();
      }());
      </script>
      {% endif %}

    {% comment %}Webibazaar Color Customizer{% endcomment %}
    {% if settings.displaycustomizer %}
    <div class="wballcor">
        <div class="wbopen-closeclr wbclrdisable"><a href="javascript:void(0)" title="{{ 'wbcustomlabel.wballtext.colors' | t }}"></a></div>
        <div class="wbcolor_box">
            <h3>Live Settings</h3>
            <p>Theme Color</p>
            <ul class="wbinnerclr">
                <li><a href="{{ 'wbcolor1.css' | asset_url }}" class="wbclr1" title="{{ 'wbcustomlabel.wballtext.colors' | t }}"></a></li>
                <li><a href="{{ 'wbcolor2.css' | asset_url }}" class="wbclr2" title="{{ 'wbcustomlabel.wballtext.colors' | t }}"></a></li>
                <li><a href="{{ 'wbcolor3.css' | asset_url }}" class="wbclr3" title="{{ 'wbcustomlabel.wballtext.colors' | t }}"></a></li>
                <li><a href="{{ 'wbcolor4.css' | asset_url }}" class="wbclr4" title="{{ 'wbcustomlabel.wballtext.colors' | t }}"></a></li>
                <li><a href="{{ 'wbcolor5.css' | asset_url }}" class="wbclr5" title="{{ 'wbcustomlabel.wballtext.colors' | t }}"></a></li>
                <li><a href="{{ 'wbcolor6.css' | asset_url }}" class="wbclr6" title="{{ 'wbcustomlabel.wballtext.colors' | t }}"></a></li>
            </ul>
            <div class="clearfix"></div>
            <hr>
            <h5>Box Layout</h5>
            <ul class="wbsitebox">
                <li class="wbboxdemo">box</li>
                <li class="wbwidedemo">Full</li>
            </ul>
            <hr>
            <h5>Mode</h5>
            <ul class="wbrtlmode">
                <li><a href="{{ 'wbrtl.css' | asset_url }}" class="wbrtlinner">RTL</a></li>
                <li><a href="#" class="wbltrinner">LTR</a></li>
            </ul>
        </div>
    </div>
    {% endif %}
    {% comment %}Webibazaar Color Customizer Over{% endcomment %}

  {% include 'wholesale-product-form' %}<!-- WPD Start -->
                {% if customer.tags.size > 0 and template != 'cart' %}
                <script>
                    document.addEventListener('DOMContentLoaded', function() {
                      let checkoutbtninterval = setInterval(function(){
                        var x=document.querySelectorAll("input[name='checkout'], button[name='checkout'], input[name='goto_pp'], button[name='goto_pp'], input[name='goto_gc'], button[name='goto_gc'], [href$='checkout']");
                        var WPD_RedirectToCart = function(e) {
                          	e.stopPropagation();
                            e.preventDefault();
                            window.location = '/cart';
                            };
                            for (var i = 0; i < x.length; i++) {
                                x[i].addEventListener('click', WPD_RedirectToCart, false);
                            }
                        },500)
                });
              </script>
               {% endif %}
         

               {% if customer.tags.size > 0 and template == 'cart' %}
                 {% include 'wcp_cart' %}
               {% endif %}
               <!-- WPD End -->
  </body>
</html>
